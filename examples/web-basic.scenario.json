{
  "schema_version": "2.0.0",
  "scenario_id": "web-example",
  "name": "Web Example Flow",
  "target": "web",
  "variables": [
    {
      "id": "start_url",
      "type": "url",
      "required": true,
      "default": "https://example.com"
    }
  ],
  "steps": [
    {
      "id": "open-example",
      "title": "Open example.com",
      "kind": "action",
      "action": "open_url",
      "input": {
        "url": "${start_url}"
      }
    },
    {
      "id": "click-more-info",
      "title": "Click More information",
      "kind": "action",
      "action": "click",
      "target": {
        "strategy": "web",
        "web": {
          "css": "a"
        }
      }
    }
  ]
}
